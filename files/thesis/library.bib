journal = {Plant molecular biology},
keywords = {Adaptation, Physiological,Adaptation, Physiological: genetics,Alcohol Oxidoreductases,Alcohol Oxidoreductases: genetics,Alcohol Oxidoreductases: metabolism,Arabidopsis,Arabidopsis: genetics,Arabidopsis: physiology,Arthrobacter,Arthrobacter: enzymology,Ascorbate Peroxidases,Catalase,Catalase: metabolism,Enzyme Activation,Enzyme Activation: drug effects,Fatty Acids,Fatty Acids: chemistry,Free Radical Scavengers,Free Radical Scavengers: metabolism,Gene Expression,Genes, Bacterial,Glycerophospholipids,Glycerophospholipids: chemistry,Hydrogen Peroxide,Hydrogen Peroxide: metabolism,Light,Peroxidases,Peroxidases: metabolism,Photosynthesis,Photosynthesis: radiation effects,Plant Leaves,Plant Leaves: drug effects,Plant Leaves: metabolism,Plant Leaves: radiation effects,Plants, Genetically Modified,Sodium Chloride,Sodium Chloride: pharmacology,Temperature},
