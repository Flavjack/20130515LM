issn = {0034-7744},
journal = {Revista de biolog\'{\i}a tropical},
keywords = {Acclimatization,Arecaceae,Arecaceae: classification,Arecaceae: embryology,Genetic,Genetic: genetics,Genetic: physiology,Germination,Germination: physiology,Hybridization,Seeds,Seeds: embryology,Species Specificity},
