file = {:C$\backslash$:/Users/Flavio Lozano Isla/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Muschak, Willmitzer, Fisahn - 1999 - Gas-exchange analysis of chloroplastic fructose-1,6-bisphosphatase antisense potatoes at differe(2).pdf:pdf},
