month = oct,
number = {10},
pages = {1343--1350},
title = {{Response of Jatropha curcas L. to water deficits: Yield, water use efficiency and oilseed characteristics}},
url = {http://www.sciencedirect.com/science/article/pii/S0961953408001542},
volume = {33},
