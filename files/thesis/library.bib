@article{Zohair2001,
abstract = {The efficiencies of acidic solutions (radish, citric acid, ascorbic acid, acetic acid and hydrogen peroxide), neutral solutions (sodium chloride) and alkaline solution (sodium carbonate) as well as tap water in the elimination of organochlorine and organophosphorus pesticides from naturally contaminated potatoes were examined. The results indicated that acidic solutions were more effective than neutral and alkaline solutions in the elimination of the organochlorine compounds under investigation, Radish solutions eliminated pesticides completely, except o,p'-DDE (73.1\% loss), followed by citric and ascorbic acid solutions. On the other hand, organophosphorus pesticides (pirimphos methyl, malathion and profenofos) were eliminated more by acidic, neutral and alkaline solutions than by organochlorines. The percentage of removal ranged from 98.5 to 100\% for pirimphos methyl, 87.9 to 100\% for malathion and 100\% for profenofos.},
