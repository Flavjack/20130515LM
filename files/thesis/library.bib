issn = {0032-0935},
journal = {Planta},
keywords = {Abscisic Acid,Abscisic Acid: metabolism,Chlorophyll,Chlorophyll: metabolism,Chromatography, High Pressure Liquid,Cyclopentanes,Cyclopentanes: metabolism,Electrophysiology,Fires,Oxylipins,Photosynthesis,Photosynthesis: physiology,Plant Growth Regulators,Plant Growth Regulators: metabolism,Plant Leaves,Plant Leaves: metabolism,Plant Leaves: physiology,Salicylates,Salicylates: metabolism,Tobacco,Tobacco: metabolism,Tobacco: physiology},
month = dec,
number = {1},
pages = {235--44},
pmid = {16773374},
title = {{Electrical and chemical signals involved in short-term systemic photosynthetic responses of tobacco plants to local burning.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16773374},
volume = {225},
