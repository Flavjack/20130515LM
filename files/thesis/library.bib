journal = {International journal of biometeorology},
keywords = {Carbon,Carbon: metabolism,Climate,Environmental Monitoring,Periodicity,Photosynthesis,Photosynthesis: physiology,Quercus,Quercus: physiology,Seasons},
month = aug,
