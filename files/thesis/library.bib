issn = {0032-0889},
journal = {Plant physiology},
month = jun,
number = {2},
pages = {423--9},
pmid = {16664832},
title = {{Chlorophyll a Fluorescence and Photosynthetic and Growth Responses of Pinus radiata to Phosphorus Deficiency, Drought Stress, and High CO(2).}},
