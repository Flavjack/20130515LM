@article{Bucksch2014a,
abstract = {Current plant phenotyping technologies to characterize agriculturally relevant traits have been primarily developed for use in laboratory and/or greenhouse conditions. In the case of root architectural traits, this limits phenotyping efforts, largely, to young plants grown in specialized containers and growth media. Hence, novel approaches are required to characterize mature root systems of older plants grown under actual soil conditions in the field. Imaging methods able to address the challenges associated with characterizing mature root systems are rare due, in part, to the greater complexity of mature root systems, including the larger size, overlap, and diversity of root components. Our imaging solution combines a field-imaging protocol and algorithmic approach to analyze mature root systems grown in the field. Via two case studies, we demonstrate how image analysis can be utilized to estimate localized root traits that reliably capture heritable architectural diversity as well as environmentally induced architectural variation of both monocot and dicot plants. In the first study, we show that our algorithms and traits (including 13 novel traits inaccessible to manual estimation) can differentiate nine maize (Zea mays) genotypes 8 weeks after planting. The second study focuses on a diversity panel of 188 cowpea (Vigna unguiculata) genotypes to identify which traits are sufficient to differentiate genotypes even when comparing plants whose harvesting date differs up to 14 d. Overall, we find that automatically derived traits can increase both the speed and reproducibility of the trait estimation pipeline under field conditions.},
annote = {From Duplicate 1 (Image-Based High-Throughput Field Phenotyping of Crop Roots - Bucksch, a.; Burridge, J.; York, L. M.; Das, a.; Nord, E.; Weitz, J. S.; Lynch, J. P.)},
author = {Bucksch, Alexander and Burridge, James and York, Larry M. and Das, Abhiram and Nord, Eric and Weitz, Joshua S. and Lynch, Jonathan P.},
doi = {10.1104/pp.114.243519},
file = {:C$\backslash$:/Users/Flavio Lozano Isla/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bucksch et al. - 2014 - Image-Based High-Throughput Field Phenotyping of Crop Roots.pdf:pdf},
issn = {0032-0889},
journal = {PLANT PHYSIOLOGY},
month = oct,
number = {October},
pages = {470--486},
pmid = {25187526},
title = {{Image-Based High-Throughput Field Phenotyping of Crop Roots}},
url = {http://www.plantphysiol.org/cgi/doi/10.1104/pp.114.243519 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4213080\&tool=pmcentrez\&rendertype=abstract},
volume = {166},
year = {2014}
}
@article{White2013,
abstract = {Crop production is often restricted by the availability of essential mineral elements. For example, the availability of N, P, K, and S limits low-input agriculture, the phytoavailability of Fe, Zn, and Cu limits crop production on alkaline and calcareous soils, and P, Mo, Mg, Ca, and K deficiencies, together with proton, Al and Mn toxicities, limit crop production on acid soils. Since essential mineral elements are acquired by the root system, the development of crop genotypes with root traits increasing their acquisition should increase yields on infertile soils. This paper examines root traits likely to improve the acquisition of these elements and observes that, although the efficient acquisition of a particular element requires a specific set of root traits, suites of traits can be identified that benefit the acquisition of a group of mineral elements. Elements can be divided into three Groups based on common trait requirements. Group 1 comprises N, S, K, B, and P. Group 2 comprises Fe, Zn, Cu, Mn, and Ni. Group 3 contains mineral elements that rarely affect crop production. It is argued that breeding for a limited number of distinct root ideotypes, addressing particular combinations of mineral imbalances, should be pursued.},
author = {White, Philip J and George, Timothy S and Dupuy, Lionel X and Karley, Alison J and Valentine, Tracy a and Wiesel, Lea and Wishart, Jane},
doi = {10.3389/fpls.2013.00193},
file = {:C$\backslash$:/Users/Flavio Lozano Isla/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/White et al. - 2013 - Root traits for infertile soils.pdf:pdf},
issn = {1664-462X},
journal = {Frontiers in plant science},
keywords = {mineral nutrition,rhizosphere,root architecture,root architecture, mineral nutrition, rhizosphere,,soil solution,uptake},
month = jan,
number = {June},
pages = {193},
pmid = {23781228},
title = {{Root traits for infertile soils.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3678079\&tool=pmcentrez\&rendertype=abstract},
volume = {4},
year = {2013}
}
