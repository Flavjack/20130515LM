issn = {0032-0889},
journal = {Plant physiology},
keywords = {Blotting, Northern,Gene Expression Regulation, Enzymologic,Gene Expression Regulation, Plant,Germination,Germination: genetics,Germination: physiology,Gibberellins,Gibberellins: biosynthesis,Mixed Function Oxygenases,Mixed Function Oxygenases: genetics,Mixed Function Oxygenases: metabolism,Oryza sativa,Oryza sativa: enzymology,Oryza sativa: genetics,Oryza sativa: growth \& development,Plant Epidermis,Plant Epidermis: cytology,Plant Epidermis: metabolism,RNA, Plant,RNA, Plant: genetics,RNA, Plant: metabolism,Seeds,Seeds: enzymology,Seeds: genetics,Seeds: growth \& development,alpha-Amylases,alpha-Amylases: biosynthesis,alpha-Amylases: genetics},
