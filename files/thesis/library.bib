journal = {Plant physiology},
keywords = {Biological Assay,Carbohydrate Metabolism,Fluorescence,Gossypium,Gossypium: growth \& development,Gossypium: metabolism,Gossypium: physiology,Heat-Shock Proteins,Heat-Shock Proteins: metabolism,Plant Leaves,Plant Leaves: metabolism,Plant Proteins,Plant Proteins: metabolism,Sucrose,Sucrose: metabolism,Time Factors,Water,Water: metabolism},
month = jan,
number = {1},
