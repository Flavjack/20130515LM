issn = {01401963},
journal = {Journal of Arid Environments},
keywords = {Biodiesel,Leaf water potential,Physic nut,Stem succulent,Stomatal conductance,Stomatal density},
month = oct,
number = {10},
pages = {877--884},
title = {{Plant–water relationships and growth strategies of Jatropha curcas L. seedlings under different levels of drought stress}},
url = {http://www.sciencedirect.com/science/article/pii/S0140196309001189},
volume = {73},
